{
  "expo": {
    "name": "cba-connect-application",
    "slug": "cba-connect-application",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cba-connect-application",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cba.cbaConnectApplication",
      "buildNumber": "3",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.cba.cba_connect_application",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pretendard-Black.otf",
            "./assets/fonts/Pretendard-Bold.otf",
            "./assets/fonts/Pretendard-ExtraBold.otf",
            "./assets/fonts/Pretendard-ExtraLight.otf",
            "./assets/fonts/Pretendard-Light.otf",
            "./assets/fonts/Pretendard-Medium.otf",
            "./assets/fonts/Pretendard-Regular.otf",
            "./assets/fonts/Pretendard-SemiBold.otf",
            "./assets/fonts/Pretendard-Thin.otf"
          ],
          "android": {
            "fonts": [
              { "fontFamily": "Pretendard-Black", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Black.otf" }] },
              { "fontFamily": "Pretendard-Bold", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Bold.otf" }] },
              { "fontFamily": "Pretendard-ExtraBold", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-ExtraBold.otf" }] },
              { "fontFamily": "Pretendard-ExtraLight", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-ExtraLight.otf" }] },
              { "fontFamily": "Pretendard-Light", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Light.otf" }] },
              { "fontFamily": "Pretendard-Medium", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Medium.otf" }] },
              { "fontFamily": "Pretendard-Regular", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Regular.otf" }] },
              { "fontFamily": "Pretendard-SemiBold", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-SemiBold.otf" }] },
              { "fontFamily": "Pretendard-Thin", "fontDefinitions": [{ "path": "./assets/fonts/Pretendard-Thin.otf" }] }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Pretendard-Black.otf",
              "./assets/fonts/Pretendard-Bold.otf",
              "./assets/fonts/Pretendard-ExtraBold.otf",
              "./assets/fonts/Pretendard-ExtraLight.otf",
              "./assets/fonts/Pretendard-Light.otf",
              "./assets/fonts/Pretendard-Medium.otf",
              "./assets/fonts/Pretendard-Regular.otf",
              "./assets/fonts/Pretendard-SemiBold.otf",
              "./assets/fonts/Pretendard-Thin.otf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
